#!/bin/bash
export $(grep -v '^#' .env.development | xargs)

export TF_VAR_payments_queue_msg_ttl=$RABBITMQ_QPAYMENTS_MSG_TTL
export TF_VAR_payments_queue_exchange_dlq=$RABBITMQ_QPAYMENTS_EXCHANGE_DLQ
export TF_VAR_payments_dlq_msg_ttl=$RABBITMQ_QPAYMENTS_DLQ_MSG_TTL

terraform -chdir=environment/terraform $@
